<% var s = window.App.S; %>
<% function toBold(s){ return "<b>"+s+"</b>";} %>

<% if(corr === undefined) { %>
	<div class="row resultSection">
		<div class="col-sm-10 col-sm-offset-1">
			<hr />
			<h2 class="compareToTitle"><%= compareToFull %></h2>
			<p>
				<%= s['notEnoughData'] %>
			</p>
		</div>
	</div>
<% } else{ %>
	<div class="row resultSection">
		<div class="col-sm-10 col-sm-offset-1">
			<hr />
			<h3 class="compareToTitle"><%= compareToFull %></h3>
		</div>
	</div>
	<div class="row resultSection">
		<div class="col-sm-5 col-sm-offset-1">
			<p class="sub-sub-title"><%= s['agreement'] %></p>
			<canvas class="corr-pie-chart-container pie-chart-container"></canvas>
		</div>
		<div class="col-sm-5">
			<p class="sub-sub-title"><%= s['hiOrLow'] %></p>
			<canvas class="percent-pie-chart-container pie-chart-container"></canvas>
		</div>
	</div>
	<br />
<% } %>
